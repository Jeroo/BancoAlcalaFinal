<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="trabajofinal.alcala.salvador.com.bancoalcala">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logobancoalcala"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LogonActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".AltaCuentasActivity" />
        <activity android:name=".IngresosActivity" />
        <activity android:name=".RetirarActivity" />
        <activity android:name=".TransferirActivity" />
        <activity android:name=".RecargarActivity" />
        <activity android:name=".MovimientosActivity" />
        <activity android:name=".SaldoActivity"></activity>
    </application>

</manifest>